[Unit]
Description=i3 Window Manager Workspace Toggling
After=network.target
                    
[Service]
User=<%= @owner %>
Group=<%= @group %>
ExecStart=/usr/local/bin/<%= @bin %> -o <%= @log_path %> -d <%= @display_num %> -i <%= @interval %>
ExecStop=/bin/kill -SIGINT $MAINPID
Restart=always
              
[Install]
WantedBy=multi-user.target
